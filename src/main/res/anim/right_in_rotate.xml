<?xml version="1.0" encoding="utf-8"?>
<set
        xmlns:android="http://schemas.android.com/apk/res/android">
    <translate
            android:duration="3500"
            android:fromXDelta="10.000002%"
            android:toXDelta="29.999996%"
            android:fromYDelta="0.0%"
            android:toYDelta="0.0%"/>
    <translate
            android:duration="3500"
            android:startOffset="350"
            android:fromXDelta="29.999996%"
            android:toXDelta="-29.999996%"
            android:fromYDelta="0.0%"
            android:toYDelta="0.0%"/>
    <scale
            android:duration="6000"
            android:pivotX="50.0%"
            android:pivotY="50.0%"
            android:fillBefore="false"
            android:fillAfter="true"
            android:fromXScale="0.6"
            android:toXScale="1.0"
            android:fromYScale="0.6"
            android:toYScale="1.0"/>
    <alpha
            android:duration="4000"
            android:fillAfter="true"
            android:fromAlpha="0.0"
            android:toAlpha="1.0"/>
    <translate
            android:duration="90000"
            android:fromXDelta="-10"
            android:toXDelta="10"
            android:repeatMode="reverse"
            android:repeatCount="infinite"/>
    <translate
            android:duration="170000"
            android:fromYDelta="-10"
            android:toYDelta="10"
            android:repeatMode="reverse"
            android:repeatCount="infinite"/>
    <!--<rotate android:duration="100000" android:repeatMode="reverse" android:repeatCount="infinite" android:fromDegrees="-5" android:toDegrees="5" android:pivotX="50%" android:pivotY="50%"/>-->
</set>